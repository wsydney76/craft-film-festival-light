{
    "name": "wsydney76/craft-film-festival-light",
    "description": "Adds film festival functionality to Craft starter",
    "type": "craft-plugin",
    "license": "mit",
    "support": {
        "issues": "https://github.com/wsydney76/craft-film-festival-light/issues?state=open",
        "source": "https://github.com/wsydney76/craft-film-festival-light",
        "docs": "https://github.com/wsydney76/craft-film-festival-light",
        "rss": "https://github.com/wsydney76/craft-film-festival-light/releases.atom"
    },
    "require": {
        "php": ">=8.0.2",
        "craftcms/cms": "^4.3.5"
    },
    "require-dev": {
        "craftcms/ecs": null,
        "craftcms/phpstan": null
    },
    "autoload": {
        "psr-4": {
            "wsydney76\\ff\\": "src/"
        }
    },
    "extra": {
        "handle": "ff-light",
        "name": "Film Festival Light",
        "developer": "wsydney76",
        "documentationUrl": "https://github.com/wsydney76"
    },
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "8.0.2"
        },
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "craftcms/plugin-installer": true
        }
    }
}

